{
  "name": "@imccausl/dev",
  "version": "1.4.1",
  "type": "module",
  "scripts": {
    "clean": "rm -rf lib",
    "compile": "tsc -b ./tsconfig.build.json",
    "copy-assets": "tsx infrastructure/copy-assets.ts",
    "build": "yarn dev build",
    "publish": "semantic-release",
    "lint": "yarn dev lint",
    "lint:fix": "yarn lint --fix",
    "format": "yarn dev format",
    "typecheck": "tsc --noEmit",
    "dev": "tsx src/index.ts"
  },
  "bin": "./lib/index.js",
  "exports": {
    "./eslint-config": "./src/lint/config/index.js",
    "./eslint-config/base": "./src/lint/config/base.js",
    "./tsdown": {
      "default": "./src/build/config/tsdown.ts"
    },
    "./vite": {
      "default": "./src/build/config/vite/index.ts"
    },
    "./prettier-config": {
      "import": {
        "default": "./src/format/config/prettier.config.js"
      },
      "require": {
        "default": "./src/format/config/prettier.config.cjs"
      },
      "default": {
        "default": "./src/format/config/prettier.config.cjs"
      }
    },
    "./tsconfig.json": "./src/types/tsconfig.default.json"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest",
    "exports": {
      "./eslint-config": {
        "types": "./lib/lint/config/index.d.ts",
        "default": "./lib/lint/config/index.js"
      },
      "./eslint-config/base": {
        "types": "./lib/lint/config/base.d.ts",
        "default": "./lib/lint/config/base.js"
      },
      "./tsdown": {
        "types": "./lib/build/config/tsdown.d.ts",
        "default": "./lib/build/config/tsdown.js"
      },
      "./vite": {
        "types": "./lib/build/config/vite/index.d.ts",
        "default": "./lib/build/config/vite/index.js"
      },
      "./prettier-config": {
        "import": "./lib/format/config/prettier.config.js",
        "require": "./lib/format/config/prettier.config.cjs",
        "default": "./lib/format/config/prettier.config.cjs"
      },
      "./tsconfig.json": "./lib/types/tsconfig.default.json"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imccausl/dev-scripts.git"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@eslint/js": "^9.30.1",
    "@imccausl/eslint-config": "^4.0.3",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@typescript-eslint/types": "^8.37.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/eslint-plugin": "^1.3.4",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^7.6.1",
    "jiti": "^2.4.2",
    "prettier": "^3.6.2",
    "tsdown": "^0.12.9",
    "typescript-eslint": "^8.36.0",
    "vite": "^7.1.5"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "prettier": "^3.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/eslint-plugin-jsx-a11y": "^6",
    "@types/node": "^22.0.0",
    "@yarnpkg/sdks": "^3.2.2",
    "semantic-release": "^24.2.6",
    "semantic-release-yarn": "^3.0.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "packageManager": "yarn@4.9.2"
}
