{
  "name": "@imccausl/dev",
  "version": "2.3.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf lib",
    "compile": "tsc -b ./tsconfig.build.json",
    "copy-assets": "tsx infrastructure/copy-assets.ts",
    "build": "yarn dev lib",
    "publish": "semantic-release",
    "lint": "yarn dev lint",
    "lint:fix": "yarn lint --fix",
    "format": "yarn dev format",
    "typecheck": "tsc --noEmit",
    "dev": "tsx src/index.ts"
  },
  "bin": "./lib/index.js",
  "exports": {
    "./semantic-release-npm-config": "./src/release/config/index.ts",
    "./eslint-config": "./src/lint/config/index.js",
    "./eslint-config/base": "./src/lint/config/base.js",
    "./tsdown": {
      "default": "./src/lib/config/tsdown.ts"
    },
    "./vite": {
      "default": "./src/build/config/vite/index.ts"
    },
    "./prettier-config": {
      "import": {
        "default": "./src/format/config/prettier.config.js"
      },
      "require": {
        "default": "./src/format/config/prettier.config.cjs"
      },
      "default": {
        "default": "./src/format/config/prettier.config.cjs"
      }
    },
    "./tsconfig.json": "./src/types/tsconfig.default.json"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest",
    "access": "public",
    "exports": {
      "./semantic-release-npm-config": {
        "default": "./lib/release/config/index.js",
        "types": "./lib/release/config/index.d.ts"
      },
      "./eslint-config": {
        "types": "./lib/lint/config/index.d.ts",
        "default": "./lib/lint/config/index.js"
      },
      "./eslint-config/base": {
        "types": "./lib/lint/config/base.d.ts",
        "default": "./lib/lint/config/base.js"
      },
      "./tsdown": {
        "types": "./lib/lib/config/tsdown.d.ts",
        "default": "./lib/lib/config/tsdown.js"
      },
      "./vite": {
        "types": "./lib/build/config/vite/index.d.ts",
        "default": "./lib/build/config/vite/index.js"
      },
      "./prettier-config": {
        "import": "./lib/format/config/prettier.config.js",
        "require": "./lib/format/config/prettier.config.cjs",
        "default": "./lib/format/config/prettier.config.cjs"
      },
      "./tsconfig.json": "./lib/types/tsconfig.default.json"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imccausl/dev-scripts.git"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@eslint/js": "^9.39.0",
    "@imccausl/eslint-config": "^4.0.9",
    "@imccausl/semantic-release-yarn": "^4.0.12",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.1",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@testing-library/dom": "^10.4.1",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@typescript-eslint/types": "^8.46.3",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/eslint-plugin": "^1.4.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-testing-library": "^7.13.3",
    "find-up": "^8.0.0",
    "jiti": "^2.4.2",
    "prettier": "^3.6.2",
    "read-package-up": "^11.0.0",
    "semantic-release": "^25.0.2",
    "tsdown": "^0.17.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.2",
    "vitest": "^4.0.6",
    "yaml": "^2.8.1"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "prettier": "^3.6.2",
    "typescript": "5.9.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/eslint-plugin-jsx-a11y": "^6",
    "@types/find-up": "^4.0.2",
    "@types/node": "^22.0.0",
    "@yarnpkg/sdks": "^3.2.3",
    "tsx": "^4.20.6"
  },
  "packageManager": "yarn@4.10.3"
}
